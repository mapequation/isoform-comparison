{
  "name": "IsoformMapper",
  "version": "0.7.0",
  "private": true,
  "homepage": "/isoformmapper",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage --watch=false",
    "release": "standard-version",
    "predeploy": "npm run build",
    "deploy": "rsync -avzh dist/* mapequation:/var/www/isoformmapper"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mapequation/c3": "^1.1.1",
    "@mapequation/infomap": "^2.7.1",
    "@mapequation/infomap-parser": "^1.0.2",
    "@mapequation/infomap-react": "^1.0.0",
    "@tanstack/react-table": "8.0.0-beta.9",
    "@types/node": "^20.11.27",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "biomsa": "^0.3.3",
    "d3": "^7.9.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^11.0.12",
    "jszip": "^3.10.1",
    "localforage": "^1.10.0",
    "lucide-react": "^0.462.0",
    "mobx": "^6.12.0",
    "mobx-react": "^9.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-force-graph-3d": "^1.24.2",
    "react-icons": "^5.0.1",
    "recharts": "^2.12.2",
    "title-case": "^4.3.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
